# Development Environment Variables for SmartVest
# Note: Only variables prefixed with VITE_ are exposed to the client-side code

# API Base URL for development (uses Vite proxy)
VITE_API_BASE_URL=http://vivekamih.gotdns.com:5501

# Application Environment
VITE_APP_ENV=development

# Enable/disable debug mode
VITE_DEBUG_MODE=true

# Application Title
VITE_APP_TITLE=SmartVest (Development)

# API Timeout (in milliseconds) - shorter for development
VITE_API_TIMEOUT=5000

# Disable analytics in development
VITE_ENABLE_ANALYTICS=false

# Application Version
VITE_APP_VERSION=1.0.0-dev

# Base URL for development
VITE_BASE_URL=/